{
    "options": {
        "cleancss":  true,
        "compress": true,
        "yuicompress": true,
        "optimization": 2,
        "banner": "/*! <%= grunt.template.today(\"yyyy-mm-dd\") %> */\n",
        "paths": [ "less" ]
    },
    "files": {
        "expand": true,
        "cwd": "less/",
        "src": "**/*.less",
        "dest": ".tmp/css/",
        "ext": ".css"
    }
}