{
    "options": {
        "name": "/lib/durandal-almond/almond",
        "inlineText": true,
        "stubModules": [ "/lib/requirejs-text/text", "text" ],
        "paths": {
            "text": "/lib/requirejs-text/text",
            "durandal": "/lib/durandal/js",
            "plugins": "/lib/durandal/js/plugins",
            "transitions": "/lib/durandal/js/transitions",
            "knockout": "/lib/knockout.js/knockout",
            "bootstrap": "/lib/bootstrap/bootstrap",
            "jquery": "/lib/jquery/jquery"
        },
        "keepBuildDir": true,
        "optimize": "none",
        "pragmas": {
            "build": true
        },
        "wrap": true,
        "baseUrl": "wwwroot/js",
        "out": "wwwroot/js/main-built.js",
        "mainPath": "wwwroot/js/ts.js",
        "mainConfigFile": "wwwroot/js/ts.js",
        "include": [
            "wwwroot/js/viewmodels/**/*.js"
        ],
        "exclude": [ ],
        "loglevel": "default",
        "includeMain": true,
        "insertRequire": [ "main" ]
    }
}