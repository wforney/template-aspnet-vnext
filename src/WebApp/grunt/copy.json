{
    "build": {
        "files": [
            {
                "src": "index.html",
                "dest": "wwwroot/"
            },
            {
                "expand": true,
                "cwd": "ng-apps/",
                "src": [
                    "<%= staticFilePattern %>"
                ],
                "dest": "wwwroot/ng-apps",
                "options": { "force": true }
            }
        ]
    }
}