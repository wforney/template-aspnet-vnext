{
    "build": {
        "options": {
            "optimizationLevel": 5,
            "svgoPlugins": [ { "removeViewBox": false } ]
        },
        "files": [
            {
                "expand": true,
                "cwd": "img/",
                "src": [ "**/*.{png,jpeg,jpg,gif,svg}" ],
                "dest": ".tmp/img/"
            }
        ]
    }
}
