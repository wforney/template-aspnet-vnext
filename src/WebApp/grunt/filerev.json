{
    "options": {
        "algorithm": "md5",
        "encoding": "utf8",
        "length": 20
    },
    "build": {
        "src": [
            ".tmp/img/**/*.{gif,jpg,jpeg,png,webp,svg}",
            ".tmp/js/**/*.js",
            ".tmp/css/**/*.css",
            ".tmp/lib/**/*.{js,css,eot,svg,ttf,woff}",
            "!.tmp/lib/durandal/**",
            "!.tmp/lib/durandal-almond/**",
            ".tmp/js/app/views/*.html"
        ]
    }
}