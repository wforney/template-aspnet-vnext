<?xml version="1.0" encoding="utf-8"?>
<!-- Please read: http://msdn.microsoft.com/en-us/library/ie/dn455106.aspx -->
<browserconfig>
  <msapplication>
    <tile>
      <square70x70logo src="http://www.tulipinn.net/img/tile-square70x70.png"/>
      <square150x150logo src="http://www.tulipinn.net/img/tile-square150x150.png"/>
      <wide310x150logo src="http://www.tulipinn.net/img/tile-wide310x150.png"/>
      <square310x310logo src="http://www.tulipinn.net/img/tile-square310x310.png"/>
      <TileColor>#FEFAC9</TileColor>
    </tile>
    <badge>
      <polling-uri src="http://www.tulipinn.net/badge.xml"/>
      <frequency>1440</frequency>
    </badge>
    <notification>
      <polling-uri src="http://www.tulipinn.net/livetile.xml"/>
      <frequency>1440</frequency>
      <cycle>1</cycle>
    </notification>
  </msapplication>
</browserconfig>
